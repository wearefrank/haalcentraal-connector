	<changeSet id="hcc-1" author="Mike Erkemeij">
		<comment>Add persoon Table</comment>
		<createTable tableName="personen2">
            <column name="burgerservicenummer" type="INT"/>
            <column name="body" type="java.sql.Types.VARCHAR()"/>
            <column name="appid" type="java.sql.Types.VARCHAR()"/>
            <column name="date" type="DATETIME"/>
        </createTable>
	</changeSet>
</databaseChangeLog>